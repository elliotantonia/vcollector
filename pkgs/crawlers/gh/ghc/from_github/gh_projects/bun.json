{
    "sdk": "bun",
    "repo": "oven-sh/bun",
    "homepage": "https://bun.sh/",
    "exclude": {
        "contains":["archive/refs/", "profile", "baseline"],
        "suffix": [".txt", ".txt.asc"],
        "prefix": []
    },
    "include": {
        "contains":[],
        "suffix": [],
        "prefix": []
    },
    "os": {
        "darwin":"darwin",
        "linux":"linux",
        "windows":"windows"
    },
    "arch": {
        "-x64":"amd64",
        "-aarch64":"arm64"
    },
    "type": "unarchiver",
    "versionRegExp": "v\\d+(.\\d+){2}",
    "install": {
        "flagfiles": {
            "windows": ["bun.exe"],
            "macos": ["bun"],
            "linux": ["bun"]
        },
        "flagDirExcepted": true,
        "binaryDir": {
            "windows": [[]],
            "macos": [[]],
            "linux": [[]]
        }
    }
}