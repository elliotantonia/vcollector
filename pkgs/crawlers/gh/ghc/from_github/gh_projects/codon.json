{
    "sdk": "codon",
    "repo": "exaloop/codon",
    "homepage": "https://github.com/exaloop/codon",
    "exclude": {
        "contains":["archive/refs/"],
        "suffix": [],
        "prefix": []
    },
    "include": {
        "contains":[],
        "suffix": [],
        "prefix": []
    },
    "os": {
        "darwin":"darwin",
        "linux":"linux",
        "windows":"windows"
    },
    "arch": {
        "-x86_64":"amd64",
        "arm64":"arm64"
    },
    "type": "unarchiver",
    "versionRegExp": "v\\d+(.\\d+){2}",
    "install": {
        "flagfiles": {
            "windows": ["bin", "lib"],
            "macos": ["bin", "lib"],
            "linux": ["bin", "lib"]
        },
        "FlagDirExcepted": false,
        "binaryDir": {
            "windows": [["bin"]],
            "macos": [["bin"]],
            "linux": [["bin"]]
        }
    }
}